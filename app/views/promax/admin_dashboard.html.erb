
<div class="hstyle">
<span class="glyphicon glyphicon-dashboard" aria-hidden="true"></span>
admin dashboard</div>

<div class="dash">
<div class="row text-center">
	<div class="col-lg-3">
		<div class="mpanel">
	  	
	  	
	  	<div class="h2 ">Projects</div>
	  	      Total : <%=@proj_count%><br>
              new : <%=@pro_new_count%><br>
              running : <%=@pro_running_count%><br>
              completed : <%=@pro_complete_count%>
      	 </div>
	  	

	</div>
	<div class="col-lg-3">
		<div class="mpanel">
		<div class="h2">Files Count</div>
	  	 <%=@attach_count%>
	    </div>
	</div>
	<div class="col-lg-3">
		<div class="mpanel">
	  	<div class="h2">User Count</div>
	  	
	  	Registered Users :<%=@user_count%><br>
	  	Employees : <%= @emp_count%><br>
	  	Managers  : <%= @mgr_count%><br>
	  	
	    </div>
	</div>
	<div class="col-lg-3">
		<div class="mpanel">
	  	<div class="h2">Messages</div>
	  	
	    </div>
	</div>
</div>

<div class="row">
	<div class="col-lg-12">
		<div class="panel">
	  	<div class="panel-heading">users track</div>
	  	<div class="panel-body">
	  	    <table class="table">
	  	    	<thead>
	  	    		<th>userid</th>
	  	    		<th>name</th>
	  	    		<th>current_sign_in</th>
	  	    		<th>last_sign_in</th>
	  	    		<th>current_logged_IP</th>
	  	    		<th>last_logged_IP</th>
	  	    	</thead>
	  	    	<tbody>
	  	    		
	  		<% @user_track.each do | u| %>
	  			<tr>
	  	    			<td><%=u.id%></td>
	  	    			<td><%=u.firstname%></td>
	  	    			<td><%=u.current_sign_in_at%></td>
	  	    			<td><%=u.last_sign_in_at%></td>
	  	    			<td><%=u.current_sign_in_ip%></td>
	  	    			<td><%=u.last_sign_in_ip%></td>
	  	    		</tr>
	  	    	
	  		<%end%>
	  		</tbody>
	  	    </table>	
	  	</div>
	    </div>
	</div>
</div>

<div class="row">
	<div class="col-lg-12">
		<div class="panel">
	  	<div class="panel-heading">Activity track</div>
	  	<div class="panel-body"></div>
	    </div>
	</div>
</div>
</div>